scalar Timestamp

type AuthPayload{
  token: String!
}

type Contact {
  ID: String
  name1: String!
  name2: String
  surname: String
  email: String
  phone: String
  website: String
  createdAt: Timestamp!
  updatedAt: Timestamp!
}

type Mutation {
  login(username: String!, password: String!): AuthPayload
  createContact(Name1: String!, Name2: String, Surname: String, Email: String, Phone: String, Website: String): Contact
}

type Query {
  getAllContacts: [Contact]
}

